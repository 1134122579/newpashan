page {
    height: 100%;
}

.detail {
    border-radius: 0 0 15rpx 15rpx;
    opacity: .83;
    padding: 20rpx 20rpx;
    top: 0;
    width: 650rpx;
}

.detail,.detail2 {
    background-color: #fff;
    left: 50%;
    position: fixed;
    transform: translate(-50%,0);
}

.detail2 {
    bottom: 0;
    padding: 30rpx;
    width: 100%;
}

.start {
    background-color: #fff;
    border-radius: 16rpx;
    left: 50%;
    position: fixed;
    top: 50%;
    transform: translate(-50%,-50%);
    width: 710rpx;
}

.startt {
    background-color: #4e63a9;
    border-radius: 16rpx 16rpx 0 0;
    color: #fff;
    font-size: 32rpx;
    line-height: 44rpx;
    padding: 24rpx 0;
    text-align: center;
}

.qrcode {
    margin: 30rpx auto 30rpx 50%;
    transform: translate(-50%,0);
}

.time {
    display: flex;
}

.cub {
    border: 2rpx solid #000;
    font-size: 26rpx;
    font-weight: 700;
    margin: 0 20rpx;
    padding: 10rpx 20rpx;
    border-radius: 5rpx;
}

.qrcode-info {
    display: flex;
    justify-content: center;
}

.share-canvas {
    width: 670rpx;
}

.btn-group {
    display: flex;
    justify-content: center;
}

.cancelbtn,.startbtn {
    background-color: #4e63a9;
    border-radius: 5rpx;
    color: #fff;
    font-size: 26rpx;
    margin: 30rpx;
    padding: 26rpx 0;
    text-align: center;
    width: 317rpx;
    border-radius: 10rpx;
}

.mask {
    left: 0;
    position: fixed;
    top: 0;
}

.detail2-options {
    display: flex;
    justify-content: space-around;
    text-align: center;
}

.detail2-options-title {
    color: #646566;
    font-family: PingFang SC;
    font-size: 32rpx;
    font-weight: 400;
    line-height: 44rpx;
    opacity: 1;
    padding: 44rpx 0;
    width: auto;
}

.detail2-line {
    background-color: #999;
    opacity: .2;
    width: 2rpx;
}

.detail2-options-data {
    color: #535353;
    display: flex;
    font-size: 32rpx;
    line-height: 44rpx;
    justify-content: center;
    align-items: center;
}

.detail2-options-data,.finish {
    font-family: PingFang SC;
    font-weight: 400;
    opacity: 1;
}

.flexContent{
    display: flex;
    justify-content: space-around;
    align-items: center;
    padding:20rpx  40rpx;
}
.button_{
    width: 44%;
}
.finish {
    /* background: #4e63a9; */
    border-radius: 1px;
    display: flex;  
    width: 100%;
    justify-content: space-around;
    align-items: center;
    color: #fff;
    /* display: inline-block; */
    font-size: 26rpx;
    margin-bottom: calc(env(safe-area-inset-bottom)40rpx);
    margin-left: 50%;
    margin-top: 58rpx;
}

.finish-box {
    border-radius: 10rpx;
    left: 50%;
    position: fixed;
    top: 50%;
    transform: translate(-50%,-50%);
    z-index: 13;
}
.detail2-options-time{
    border: 1px solid #347DD7;
    border-radius: 50%;
    width: 200rpx;
    height: 200rpx;
    text-align: center;
}

.close {
    left: 10rpx;
}
.close,.share {
    height: 72rpx;
    position: absolute;
    top: 10rpx;
    width: 72rpx;
    z-index: 19;
}

.share {
    right: 10rpx;
}

.fold {
    color: #787878;
    font-size: 24rpx;
    font-weight: 400;
}

.reportstfd {
    color: #ff3636;
}

.currentstf,.reportstfd {
    display: flex;
    font-size: 26rpx;
    font-weight: 400;
    line-height: 2;
}

.currentstf {
    color: #3c9d63;
}

.entrystf {
    display: flex;
}

.entrystf,.report {
    font-size: 26rpx;
    font-weight: 400;
}

.report {
    background-color: #ff3636;
    color: #fff;
    display: inline-block;
    padding: 20rpx 30rpx;
    box-sizing: border-box;
    border-radius: 10rpx;
}
.header_xx{
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.jyObj{
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    font-size: 18px;
    font-weight: 600;
}
.JyObjpl{
    font-size: 14px;
    font-weight: normal;
    color: #ccc;
}